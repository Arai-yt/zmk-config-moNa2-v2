&zip_mouse_gesture {
    stroke-size = <300>;
    movement-threshold = <8>;
    idle-timeout-ms = <200>;
    gesture-cooldown-ms = <120>;
    enable-eager-mode;

    prev_tab  { pattern = <GESTURE_LEFT>;  bindings = <&kp A>; };
    next_tab  { pattern = <GESTURE_RIGHT>; bindings = <&kp D>; };
    new_tab   { pattern = <GESTURE_UP>;    bindings = <&kp W>; };
    close_tab { pattern = <GESTURE_DOWN>;  bindings = <&kp S>; };
};
